# SwasthVault Frontend Development Environment Variables
# Copy this to .env.local for local development

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Environment
REACT_APP_ENV=development

# Debug Configuration
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Feature Flags
REACT_APP_ENABLE_MOCK_DATA=true
REACT_APP_ENABLE_DEV_TOOLS=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false
REACT_APP_ENABLE_ERROR_BOUNDARY=true

# External Services (Development)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_for_dev
REACT_APP_ANALYTICS_ID=your_analytics_id_for_dev
REACT_APP_SENTRY_DSN=your_sentry_dsn_for_dev

# Development URLs
REACT_APP_BACKEND_URL=http://localhost:5000
REACT_APP_WEBSOCKET_URL=ws://localhost:5000
REACT_APP_FRONTEND_URL=http://localhost:3000

# Theme Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_ENABLE_DARK_MODE=true
REACT_APP_ENABLE_THEME_SWITCHER=true

# Development Features
GENERATE_SOURCEMAP=true
FAST_REFRESH=true
REACT_APP_ENABLE_HOT_RELOAD=true

# Authentication Configuration
REACT_APP_JWT_STORAGE_KEY=swasthvault_token
REACT_APP_REFRESH_TOKEN_KEY=swasthvault_refresh_token
REACT_APP_TOKEN_EXPIRY_BUFFER=300000

# API Configuration
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_ATTEMPTS=3
REACT_APP_API_RETRY_DELAY=1000

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=.pdf,.jpg,.jpeg,.png,.doc,.docx
REACT_APP_UPLOAD_CHUNK_SIZE=1048576

# Pagination Configuration
REACT_APP_DEFAULT_PAGE_SIZE=10
REACT_APP_MAX_PAGE_SIZE=100

# Cache Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_ENABLE_OFFLINE_MODE=true

# Notification Configuration
REACT_APP_NOTIFICATION_TIMEOUT=5000
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false

# Development Mock Data
REACT_APP_MOCK_DELAY=500
REACT_APP_MOCK_ERROR_RATE=0.1
REACT_APP_ENABLE_MOCK_AUTH=false

# Performance Configuration
REACT_APP_ENABLE_LAZY_LOADING=true
REACT_APP_ENABLE_CODE_SPLITTING=true
REACT_APP_BUNDLE_ANALYZER=false

# Security Configuration (Development)
REACT_APP_ENABLE_CSP=false
REACT_APP_ENABLE_HTTPS_ONLY=false

# Accessibility Configuration
REACT_APP_ENABLE_A11Y_CHECKS=true
REACT_APP_A11Y_LEVEL=AA

# Internationalization
REACT_APP_DEFAULT_LOCALE=en
REACT_APP_ENABLE_I18N=true
REACT_APP_SUPPORTED_LOCALES=en,hi,ta,te,bn

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_REACT_DEVTOOLS=true
REACT_APP_ENABLE_PROFILER=true

# Error Handling
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_ERROR_BOUNDARY_FALLBACK=true

# Testing Configuration
REACT_APP_ENABLE_TEST_IDS=true
REACT_APP_TEST_USER_EMAIL=test@swasthvault.com
REACT_APP_TEST_USER_PASSWORD=test123

# Development Flags
REACT_APP_SKIP_PREFLIGHT_CHECK=true
REACT_APP_DISABLE_ESLINT_PLUGIN=false

# Build Configuration
REACT_APP_BUILD_VERSION=development
REACT_APP_BUILD_DATE=auto
REACT_APP_GIT_COMMIT=auto

# Development Notes:
# - These are development-only settings
# - Use secure configurations in production
# - Never expose sensitive data in frontend environment variables
# - All REACT_APP_ variables are exposed to the browser
# - Use .env.local for local overrides (not committed to git)
